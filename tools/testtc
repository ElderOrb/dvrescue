#!/bin/bash

_convert_hhmmssmmm2s(){
    TS="${1}"
    echo "$TS" | awk -F ":" '{ printf "%8.6f\n", ($1 * 3600) + ($2 * 60) + $3 }'
}

_convert_hhmmssmmm2s "01:12:23.678"
_convert_hhmmssmmm2s "01:12:23.678"
